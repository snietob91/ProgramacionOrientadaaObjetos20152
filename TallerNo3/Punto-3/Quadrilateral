
package taller3.pkg1;

public class Quadrilateral { //Iniciaci贸n de la clase Quadrilateral
    private int x,y;
    
    public Quadrilateral(int x,int y){//Iniciaci贸n de los valores de x y y para cualquier subclase de figura
        
        this.x=x;
        this.y=y;
    }

    public int getX(){return x;} //Operaci贸n que muestra el valor de x      
    public int getY(){return y;}// Operaci贸n que muestra el valor de y
}
